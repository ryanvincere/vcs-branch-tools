{
	"manifest_version": 2,
	"name": "VCS Branch Tools",
	"description": "Create VCS branch naming from project management",
	"version": "0.2",
	"author": "Ryan Adhitama",
	"browser_action": {
		"default_title": "VCS Branch Tools",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["jquery.1.8.2.js"]
	}],
	"permissions": [
		"activeTab",
		"tabs"
	]
}